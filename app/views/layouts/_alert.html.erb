<% if notice %>
<el-row class="d-flex justify-content-center align-items-center">
    <el-col :span="8">
        <el-alert title="<%= notice %>" type="success" show-icon>
        </el-alert>
    </el-col>
</el-row>
<% elsif alert %>
<el-row class="d-flex justify-content-center align-items-center">
    <el-col :span="8">
        <el-alert title="<%= alert %>" type="warning" show-icon>
        </el-alert>
    </el-col>
</el-row>
<% end %>